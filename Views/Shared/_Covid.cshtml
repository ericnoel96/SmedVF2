@model IEnumerable<Smed.Models.Covid>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Estado.Tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Paciente.Cedula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaIngreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Medicamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tratamiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaSalida)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Estado.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Paciente.Cedula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaIngreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observacion)
        </td>        
        <td>
            @Html.DisplayFor(modelItem => item.Medicamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tratamiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaSalida)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CovidId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CovidId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CovidId })
        </td>
    </tr>
}

</table>
